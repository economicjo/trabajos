    "ajv": "8.11.0",
    "ajv-errors": "3.0.0",
    "ajv-formats": "2.1.1",
    "axios": "0.27.2",
    "jwt-decode": "3.1.2",
    "keycloak-js": "18.0.0",
    "pinia": "2.0.14",
    "primeflex": "3.1.3",
    "primeicons": "5.0.0",
    "primevue": "3.12.6",
    "sass": "1.51.0",
